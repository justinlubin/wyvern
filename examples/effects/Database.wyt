import wyvern.option
import effects.DatabaseEffects

resource type Database
  def query(
    queryString : String
  ) : {DatabaseEffects.query} option.Option[String]
  def update(
    queryString : String
  ) : {DatabaseEffects.update} Unit
  def queryAndUpdate(
    queryString : String
  ) : {DatabaseEffects.query, DatabaseEffects.update} String
