import wyvern.option
import effects.MemoryEffects

resource type Memory
  def store(
    x : Int, s : String
  ) : {MemoryEffects.store} Unit
  def load(
    x : Int
  ) : {MemoryEffects.load} option.Option[String]
  def modify(
    x : Int, f : option.Option[String] => String
  ) : {MemoryEffects.load, MemoryEffects.store} Unit
