resource type File
  effect readEffect
  effect writeEffect
  effect appendEffect
  effect createEffect

  def read(f : String) : {readEffect} String
  def write(f : String, s : String) : {writeEffect} Unit
  def append(f : String, s : String) : {appendEffect} Unit
  def create(f : String) : {createEffect} Unit
  def exists(f : String) : Boolean
